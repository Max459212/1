    "from scipy.integrate import quad\n",
    "\n",
    "# Р’РёР·РЅР°С‡Р°С”РјРѕ С„СѓРЅРєС†С–СЋ f, СЏРєР° РѕР±С‡РёСЃР»СЋС” РІРёСЂР°Р· 4 - x^2\n",
    "def f(x):\n",
    "    return 4 - x**2\n",
    "\n",
    "# Р’РёРєРѕРЅСѓС”РјРѕ С‡РёСЃРµР»СЊРЅРµ РѕР±С‡РёСЃР»РµРЅРЅСЏ РІРёР·РЅР°С‡РµРЅРѕРіРѕ С–РЅС‚РµРіСЂР°Р»Сѓ С„СѓРЅРєС†С–С— f\n",
    "# Р†РЅС‚РµРіСЂСѓС”РјРѕ РїРѕ x РІС–Рґ 0 РґРѕ 2\n",
