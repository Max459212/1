    "#Quadrature with 2 points (Trapezoidal rule)\n",
    "X = np.array([-0.5,1.5])\n",
    "Quadrature( f, X, xmin=-1, xmax=2, ymin=0, ymax=4 )\n",
    "\n",
    "def trapezia(f, a, b):\n",
    "    \"\"\"РџСЂР°РІРёР»Рѕ С‚СЂР°РїРµС†iР№\"\"\"\n",
    "    h = (b - a)\n",
    "    sum = 0.5 * (f(a) + f(b))\n",
    "    t1=(a+b)/2\n",
    "    d = - (h**3)/12*derivative(f, t1, dx=1e-6, n=10)    \n",
    "    return sum * h, d  \n",
    "\n",
    "print('Р—РЅР°С‡РµРЅРЅСЏ С–РЅС‚РµРіСЂР°Р»Сѓ = %.5f\\nРџРѕС…РёР±РєР° С–РЅС‚РµРіСЂСѓРІР°РЅРЅСЏ = %.5f' %trapezia(f1, -0.5, 1.5))"
   ]
