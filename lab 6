   "source": [
    "#Quadrature with 3 points (Simpson's rule)\n",
    "X = np.array([-0.5,0.5,1.5])\n",
    "Quadrature( f, X, xmin=-1, xmax=2, ymin=0, ymax=4 )\n",
    "\n",
    "def simpson(f, a, b):\n",
    "    h = (b - a)/2\n",
    "    sum = f(a) +4*f(a + h)+f(b)\n",
    "    t1=(a+b)/2\n",
    "    d =- h**5/90*8*(np.cos(t1)**2-np.sin(t1)**2) #Р—Р°Р»РёС€РєРѕРІРёР№ С‡Р»РµРЅ\n",
    "    return sum * h / 3, d\n",
    "\n",
    "print('Р—РЅР°С‡РµРЅРЅСЏ С–РЅС‚РµРіСЂР°Р»Сѓ = %.5f\\nРџРѕС…РёР±РєР° С–РЅС‚РµРіСЂСѓРІР°РЅРЅСЏ = %.5f' %simpson(f1, -0.5, 1.5))"
   ]
  },
