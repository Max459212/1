    "#Quadrature with 3 points (Simpson's rule)\n",
    "CompositeQuadrature( f, a=-0.5, b=1.5, N=5, n=2, xmin=-1, xmax=2, ymin=0, ymax=4 )\n",
    "\n",
    "def uzsimpson(f, a, b, N):\n",
    "    h = (b - a) / N\n",
    "    sum = f(a) + f(b)\n",
    "    t1=(a+b)/2\n",
    "    d = - ((h**4)*(b-a)/180)*8*(np.cos(t1)**2-np.sin(t1)**2) #Р—Р°Р»РёС€РєРѕРІРёР№ С‡Р»РµРЅ\n",
    "    for j in range(1, N):\n",
    "        if j%2==1:\n",
    "            sum += 4*f(a + j * h)\n",
    "    for j in range(1, N-1):\n",
    "        if j%2==0:\n",
    "            sum += 2*f(a + j * h)\n",
    "    print('РџРѕС…РёР±РєР° С–РЅС‚РµРіСЂСѓРІР°РЅРЅСЏ =',d)\n",
