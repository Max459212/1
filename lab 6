   "cell_type": "code",
   "execution_count": 17,
   "id": "d2094da6-174c-4eac-b63b-fc6caae75dc5",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Р§РёСЃРµР»СЊРЅР° РєРІР°РґСЂР°С‚СѓСЂР° \n",
    "def Quadrature( f, X, xmin, xmax, ymin=0, ymax=1, fig=None, leg=True ):\n",
    "    \n",
    "    Y = f( X ) # РІРёР·РЅР°С‡Р°С”РјРѕ С‚РѕС‡РєРё РґР»СЏ РїРѕР±СѓРґРѕРІРё С‚СЂР°РїРµС†С–С—\n",
    "    \n",
    "    # РјР°СЃРёРІ С…-С–РІ\n",
    "    Xarray = np.linspace( xmin, xmax, 1000 )\n",
    "    # РјР°СЃРёРІ Сѓ РјРµР¶Р°С… СЏРєРѕРіРѕ Р·РЅР°С…РѕРґРёС‚СЊСЃСЏ РїР»РѕС‰Р°\n",
    "    Xarea = np.linspace( X[0], X[-1], 1000 )\n",
    "    # РјР°СЃРёРІ y-С–РІ\n",
    "    Yarray = f( Xarray )\n",
    "    \n",
    "    # РІРёР·РЅР°С‡Р°С”РјРѕ РїРѕР»С–РЅРѕРј Р›Р°РіСЂР°РЅР¶Р° \n",
    "    Ln = interp.lagrange( X, Y )\n",
    "    # СѓСЃСЏ С–РЅС‚РµСЂРїРѕР»СЊРѕРІР°РЅР° РѕР±Р»Р°СЃС‚СЊ Р·РЅР°С‡РµРЅСЊ \n",
    "    Parray = Ln( Xarray )\n",
    "    # С–РЅС‚РµСЂРїРѕР»СЊРѕРІР°РЅР° РїС–РґРѕР±Р»Р°СЃС‚СЊ РґР»СЏ Р·РЅР°С…РѕРґР¶РµРЅРЅСЏ РїР»РѕС‰С– \n",
    "    Parea = Ln( Xarea )\n",
    "    \n",
    "    # СЏРєС‰Рѕ РІРёР±РёСЂР°С”РјРѕ РїРѕР±СѓРґРѕРІРё СЂРёСЃСѓРЅРєСѓ\n",
    "    if fig==None:\n",
    "        fig = plt.figure( figsize = (8,8) )\n",
    "    ax = fig.add_subplot(111)\n",
    "    # Р±СѓРґСѓС”РјРѕ С„СѓРЅРєС†С–СЋ\n",
    "    ax.plot( Xarray, Yarray, linewidth = 3, color = \"blue\", label=\"$f(x)$\" )\n",
    "    # Р±СѓРґСѓС”РјРѕ С‚РѕС‡РєРё РґР»СЏ РєРІР°РґСЂР°С‚СѓСЂРё\n",
    "    ax.plot( X, Y, \"o\", color=\"red\", label=\"points\", zorder = 10 )\n",
    "    # Р±СѓРґСѓС”РјРѕ С–РЅС‚РµСЂРїРѕР»СЏРЅС‚\n",
    "    ax.plot( Xarray, Parray, linewidth = 2, color = \"black\", label=\"$P_{%d}(x)$\"%(len(X)-1) )\n",
    "    # Р±СѓРґСѓС”РјРѕ РїР»РѕС‰Сѓ\n",
    "    ax.fill_between( Xarea, Parea, color=\"green\", alpha=0.5 )\n",
    "    \n",
    "    # РІРёСЃС‚Р°РІР»СЏС”РјРѕ РЅР°Р»Р°С€С‚СѓРІР°РЅРЅСЏ СЂРёСЃСѓРЅРєР°\n",
    "    ax.set_title( \"%d-point Quadrature\"%(len(X)), fontsize=16 )\n",
    "    ax.set_xlim( (xmin, xmax) )\n",
    "    ax.set_ylim( (0, 4) )\n",
    "    ax.set_xlabel( \"$x$\" )\n",
    "    ax.set_ylabel( \"$y$\" )\n",
    "    if leg:\n",
    "        ax.legend( loc=\"upper left\", fontsize=16 )\n",
    "    ax.grid(1)\n",
    "    \n",
    "    return ax"
   ]
