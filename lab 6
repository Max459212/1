    "from sympy import *\n",
    "x,y,a,b = symbols('x y a b') # РІРёР·РЅР°С‡Р°С”РјРѕ С–Рј'СЏ Р·РјС–РЅРЅРѕС—, С‚Р° РІРµСЂС…РЅСЋ С– РЅРёР¶РЅСЋ РіСЂР°РЅРёС†С– С–РЅС‚РµРіСЂСѓРІР°РЅРЅСЏ\n",
