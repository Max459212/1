   "source": [
    "def f(x):\n",
    "    return x**2+2*x+3\n",
    "\n",
    "def f1(x):\n",
    "    return x+np.cos(x)**2+x**2\n",
    "\n",
    "def f2(x):\n",
    "    return 1+np.cos(x)**2+x\n",
    "\n",
    "f_scipy = lambda x,y: x**2+y**2\n",
    "\n",
    "def f_scipy2(x, a, b,c):\n",
    "    return a*x**2+b*x+c"
