   "source": [
    "# Р¤СѓРЅРєС†С–СЏ РґР»СЏ РѕР±С‡РёСЃР»РµРЅРЅСЏ РїРѕС…С–РґРЅРѕС— Р·Р° РґРѕРїРѕРјРѕРіРѕСЋ РїСЂСЏРјРёС… СЂС–Р·РЅРёС†СЊ\n",
    "def forward_diff(fct, x, h=1E-6):\n",
    "    return (fct(x + h) - fct(x)) / h\n",
    "\n",
    "# Р¤СѓРЅРєС†С–СЏ РґР»СЏ РѕР±С‡РёСЃР»РµРЅРЅСЏ РїРѕС…С–РґРЅРѕС— Р·Р° РґРѕРїРѕРјРѕРіРѕСЋ С†РµРЅС‚СЂР°Р»СЊРЅРёС… СЂС–Р·РЅРёС†СЊ\n",
    "def central_diff(fct, x, h=1E-6):\n",
    "    return (fct(x + h) - fct(x - h)) / (2 * h)\n",
    "\n",
    "# РўРѕС‡РЅРµ Р·РЅР°С‡РµРЅРЅСЏ РїРѕС…С–РґРЅРѕС—\n",
    "exact = -6.0\n",
    "x = -2\n",
    "\n",
    "# Р¤СѓРЅРєС†С–СЏ РґР»СЏ РѕР±С‡РёСЃР»РµРЅРЅСЏ РїРѕС…С–РґРЅРѕС—\n",
    "def my_func(x):\n",
    "    return (x**2 - 5) * (x**3 - 2 * x + 3)\n",
    "\n",
    "print(\"РўРѕС‡РЅРµ Р·РЅР°С‡РµРЅРЅСЏ РїРѕС…С–РґРЅРѕС—: \", exact)\n",
    "print(\"%20s%40s\" % (\"РџСЂСЏРјР° СЂС–Р·РЅРёС†СЏ\", \"Р¦РµРЅС‚СЂР°Р»СЊРЅР° СЂС–Р·РЅРёС†СЏ\"))\n",
    "\n",
    "# РњР°СЃРёРІРё РґР»СЏ Р·Р±РµСЂРµР¶РµРЅРЅСЏ Р·РЅР°С‡РµРЅСЊ РїРѕС…РёР±РѕРє С‚Р° РєСЂРѕРєС–РІ h\n",
    "fd_errors = []\n",
    "cd_errors = []\n",
    "h_all = []\n",
    "\n",
    "# РџРѕС‡Р°С‚РєРѕРІРёР№ РєСЂРѕРє h\n",
    "h = 1.0\n",
    "for i in range(10):\n",
    "    fd = forward_diff(my_func, x, h)\n",
    "    cd = central_diff(my_func, x, h)\n",
    "    print(\"%10g (РїРѕС…РёР±РєР°=%10.2g)         %10g (РїРѕС…РёР±РєР°=%10.2g)\" % (fd, abs(fd - exact), cd, abs(cd - exact)))\n",
    "    h_all.append(h)\n",
    "    fd_errors.append(abs(fd - exact))\n",
    "    cd_errors.append(abs(cd - exact))\n",
    "    h = h / 2.0  # Р·РјРµРЅС€СѓС”РјРѕ РєСЂРѕРє h\n",
    "\n",
    "# РџРѕР±СѓРґРѕРІР° РіСЂР°С„С–РєСѓ РїРѕС…РёР±РѕРє\n",
    "plt.loglog(h_all, fd_errors, 'b.-', label='РџСЂСЏРјР° СЂС–Р·РЅРёС†СЏ')\n",
    "plt.loglog(h_all, cd_errors, 'k.-', label='Р¦РµРЅС‚СЂР°Р»СЊРЅР° СЂС–Р·РЅРёС†СЏ')\n",
    "plt.xlabel('h')\n",
    "plt.ylabel('РџРѕС…РёР±РєР°')\n",
    "plt.grid(True)\n",
    "plt.legend(loc='best')\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "4ea49ad7-3156-4447-869c-0fef8232d5e9",
   "metadata": {},
   "source": [
    "## <span style=\"color:blue\">Р’РїСЂР°РІР° 3: РќР°РїРёС€С–С‚СЊ РєРѕРґ РґР»СЏ РїРѕР±СѓРґРѕРІРё С–РЅС‚РµСЂРїРѕР»СЏС†С–Р№РЅРѕРіРѕ РїРѕР»С–РЅРѕРјСѓ РќСЊСЋС‚РѕРЅР° РїРѕСЂСЏРґРєСѓ $ N $, С‰Рѕ РїСЂРѕС…РѕРґРёС‚СЊ С‡РµСЂРµР· $ N + 1 $ С‚РѕС‡РєРё. Р—Р° РЅРѕРјРµСЂРѕРј РІР°СЂС–Р°РЅС‚Сѓ РѕР±РµСЂС–С‚СЊ С‚Р°Р±Р»РёС†СЋ Р·РЅР°С‡РµРЅСЊ Р°СЂРіСѓРјРµРЅС‚Р° С‚Р° С„СѓРЅРєС†С–С—</span>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 57,
   "id": "e22b5938-73c7-4aca-8523-3b14966f6487",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 640x480 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Р’РёР·РЅР°С‡Р°С”РјРѕ С‚РѕС‡РєРё\n",
    "data = np.array([\n",
    "    [-3, 7],\n",
    "    [-1, -1],\n",
    "    [3, 4],\n",
    "    [5, 5],\n",
    "    [8, -4]\n",
    "])\n",
    "\n",
    "def divided_difference(x, y, N=50):\n",
    "    \"\"\"РћР±С‡РёСЃР»РёРјРѕ N-С‚Сѓ СЃРєС–РЅС‡РµРЅСѓ СЂС–Р·РЅРёС†СЋ Р·Р° РґРѕРїРѕРјРѕРіРѕСЋ *x* С‚Р° *y*\"\"\"\n",
    "    if N == 0:\n",
    "        raise Exception(\"Р”РѕСЃСЏРіРЅСѓС‚Р° РјРµР¶Р° СЂРµРєСѓСЂСЃС–С—!\")\n",
    "    \n",
    "    # Р”РѕСЃСЏРіРЅСѓС‚Рѕ РєС–РЅС†СЏ СЂРµРєСѓСЂСЃС–С—\n",
    "    if y.shape[0] == 1:\n",
    "        return y[0]\n",
    "    elif y.shape[0] == 2:\n",
    "        return (y[1] - y[0]) / (x[1] - x[0])\n",
    "    else:\n",
    "        return (divided_difference(x[1:], y[1:], N=N-1) - divided_difference(x[:-1], y[:-1], N=N-1)) / (x[-1] - x[0])\n",
    "\n",
    "def newton_basis(x, x0):\n",
    "    \"\"\"РџРѕР±СѓРґСѓРІР°С‚Рё РЅСЊСЋС‚РѕРЅС–РІ Р±Р°Р·РёСЃ РґР»СЏ С–РЅС‚РµСЂРїРѕР»СЏС†С–Р№РЅРѕРіРѕ РїРѕР»С–РЅРѕРјР°, Р·Р°РґР°РЅРѕРіРѕ С–РЅС‚РµСЂРїРѕР»СЏС†С–Р№РЅРёРјРё С‚РѕС‡РєР°РјРё x0\"\"\"\n",
    "    basis = np.ones((len(x0), len(x)))\n",
    "    for j in range(len(x0)):\n",
    "        for i in range(j):\n",
    "            basis[j, :] *= (x - x0[i])\n",
    "    return basis\n",
    "\n",
    "def P_newton(x, data):\n",
    "    \"\"\"РџРѕР±СѓРґСѓРІР°С‚Рё С–РЅС‚РµСЂРїРѕР»СЏС†С–Р№РЅРёР№ РїРѕР»С–РЅРѕРј РїРѕСЂСЏРґРєСѓ N С‡РµСЂРµР· РґР°РЅС– N + 1 С‚РѕС‡РєРё, РІРёРєРѕСЂРёСЃС‚РѕРІСѓСЋС‡Рё Р±Р°Р·РёСЃ РќСЊСЋС‚РѕРЅР°\"\"\"\n",
    "    P = np.zeros(x.shape)\n",
    "    basis = newton_basis(x, data[:, 0])\n",
    "    for j in range(data.shape[0]):\n",
    "        P += divided_difference(data[:j + 1, 0], data[:j + 1, 1]) * basis[j, :]\n",
    "        \n",
    "    return P\n",
    "\n",
    "# Р“РµРЅРµСЂСѓС”РјРѕ С‚РѕС‡РєРё РґР»СЏ РїРѕР±СѓРґРѕРІРё РіСЂР°С„С–РєР°\n",
    "x_values = np.linspace(-4, 9, 100)  # Р“РµРЅРµСЂСѓС”РјРѕ С‚РѕС‡РєРё РІ Р·Р°РґР°РЅРѕРјСѓ РїСЂРѕРјС–Р¶РєСѓ\n",
    "y_values = P_newton(x_values, data)  # РћР±С‡РёСЃР»СЋС”РјРѕ Р·РЅР°С‡РµРЅРЅСЏ РїРѕР»С–РЅРѕРјР°\n",
    "\n",
    "# Р’С–Р·СѓР°Р»С–Р·Р°С†С–СЏ\n",
    "plt.scatter(data[:, 0], data[:, 1], color='red', label='Р”Р°РЅС– С‚РѕС‡РєРё')  # РџРѕС‡Р°С‚РєРѕРІС– С‚РѕС‡РєРё\n",
    "plt.plot(x_values, y_values, label='РџРѕР»С–РЅРѕРј РќСЊСЋС‚РѕРЅР°', color='blue')  # РџРѕР»С–РЅРѕРј\n",
    "plt.title('Р†РЅС‚РµСЂРїРѕР»СЏС†С–Р№РЅРёР№ РїРѕР»С–РЅРѕРј РќСЊСЋС‚РѕРЅР°')\n",
    "plt.xlabel('X')\n",
    "plt.ylabel('Y')\n",
    "plt.legend()\n",
    "plt.grid()\n",
    "plt.show()"
   ]
  },
