   "source": [
    "import numpy as np\n",
    "import scipy.optimize as scop\n",
    "\n",
    "# Р¤СѓРЅРєС†С–СЏ РґР»СЏ Р±С–СЃРµРєС†С–С—\n",
    "def f(x):\n",
    "    return x**3 + 2*x - 13  # Р—Р°РґР°С”РјРѕ С„СѓРЅРєС†С–СЋ\n",
    "\n",
    "# Р РµР°Р»С–Р·Р°С†С–СЏ Р°Р»РіРѕСЂРёС‚РјСѓ Р±С–СЃРµРєС†С–С—\n",
    "def bisection(fct, a, b, atol=1.0E-6, nmax=100):\n",
    "    n = 0\n",
    "    while n <= nmax:\n",
    "        c = (a + b) / 2.  # РЎРµСЂРµРґРёРЅР° С–РЅС‚РµСЂРІР°Р»Сѓ\n",
    "        if fct(c) == 0. or (b - a) / 2. < atol:  # РЈРјРѕРІРё РІРёС…РѕРґСѓ\n",
    "            return c\n",
    "        n += 1\n",
    "        if np.sign(fct(c)) == np.sign(fct(a)):\n",
    "            a = c  # Р—РјС–РЅСЋС”РјРѕ Р»С–РІСѓ РјРµР¶Сѓ\n",
    "        else:\n",
    "            b = c  # Р—РјС–РЅСЋС”РјРѕ РїСЂР°РІСѓ РјРµР¶Сѓ\n",
    "    raise RuntimeError('no root found within [a,b]')  # РџРѕРјРёР»РєР°, СЏРєС‰Рѕ РєРѕСЂС–РЅСЊ РЅРµ Р·РЅР°Р№РґРµРЅРѕ\n",
    "\n",
    "# Р’РёР·РЅР°С‡Р°С”РјРѕ РјРµР¶С– С–РЅС‚РµСЂРІР°Р»Сѓ\n",
    "a, b = 0.1, 3.\n",
    "print(bisection(f, a, b))  # Р’РёРєРѕРЅР°РЅРЅСЏ РјРµС‚РѕРґСѓ Р±С–СЃРµРєС†С–С—\n",
    "print(scop.bisect(f, a, b))  # РџРѕСЂС–РІРЅСЏРЅРЅСЏ Р· СЂРµР·СѓР»СЊС‚Р°С‚РѕРј scipy\n"
   ]
  },
