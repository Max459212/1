   "cell_type": "code",
   "execution_count": 51,
   "id": "5eb8add1-baef-4d2f-b589-28832ac05a1e",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2.069343781471253\n",
      "2.0693439333919295\n"
